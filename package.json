{
  "devDependencies": {
    "@types/jquery": "^3.5.6",
    "@types/lz-string": "^1.3.32",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "base64-inline-loader": "~1.1.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "~1.0.0",
    "node-sass": "^5.0.0",
    "lz-string": "^1.4.4",
    "mocha": "^5.2.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~10.0.5",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^4.2.0-beta",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  },
  "name": "pull-request-search",
  "private": true,
  "version": "0.6.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --rev-version --manifests vss-extension.json --overrides-file configs/dev.json",
    "publish-extension": "tfx extension publish --rev-version --manifests vss-extension.json --overrides-file configs/release.json"
  },
  "dependencies": {
    "azure-devops-extension-api": "^1.157.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^2.167.14",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  }
}
